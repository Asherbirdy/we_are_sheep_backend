									"",
									"if (accessToken) {",
									"    // 將 accessTokenJWT 添加到 Authorization Header 中",
									"    pm.request.headers.add({",
									"        key: \"Authorization\",",
									"        value: `Bearer ${accessToken}`",
									"    });",
									"    console.log(\"Authorization header set with token:\", accessToken);",
									"} else {",
									"    console.log(\"No accessTokenJWT found in cookies\");",
									"}",
